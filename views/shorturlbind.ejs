    <script type="text/javascript">
        $(document).ready(function () {
            $("#preview").hide();
            $('#polo').on("click", function () {
                var name = $(this).html();
                console.log(name);
                if (name === "see preview") {
                    $(this).html("hide preview");
                } else if (name === "hide preview") {
                    $(this).html("see preview");
                }
                $(".c-navbar").toggle();
                $("#preview").slideToggle();
                $('.whole').toggle();
            });
            // var dbdata1 = <%- JSON.stringify(socialmediaList) %>;
            // console.table(dbdata1);
            // for (let index = 0; index < dbdata1.length; index++) {


            // }
            var fullData = <%- JSON.stringify(socialmediaList) %>;
            // console.log(fullData);
            var sendeddata = <%-JSON.stringify(sociallists) %>;
            // console.log(sendeddata);




            // console.log(fullData.length);
            for (var i = fullData.length - 1; i >= 0; i--) {
                for (var j = 0; j < sendeddata.length; j++) {
                    // console.log(i);
                    // console.log(j);
                    // console.log(fullData[i]);
                    // console.log(sendeddata[j]);
                    // console.log(fullData[i].name);
                    // console.log(sendeddata[j].name);
                    if (fullData[i] && (fullData[i].name === sendeddata[j].name)) {
                        fullData.splice(i, 1);

                    }
                }
            }

            // alert(JSON.stringify(myArray));
            for (var j = 0; j < fullData.length; j++) {

                var name = fullData[j].name;
                var id = fullData[j]._id;
                var icon = fullData[j].icon;
                // console.log(name);
                // console.log(id);
                // console.log(icon);
                var adddata = sendsocial(name, id, icon);
                $(".dropping").append(adddata);
            }

            function sendsocial() {

                var $allData =
                    ` <a class="c-dropdown__item dropdown-item form-check form-check-inline hello" href="#" style="padding:4px 5px 0 5px;">
        <label class="iconbox1">
            <i class="` +
                    icon +
                    `" id="dropdownicon" ></i>
            </label>
    <span class="naming" style="display:none">` +
                    name +
                    `</span>
            <label class="form-check-label mx-auto" for="inlineCheckbox1" style="margin-left:8px;font-size:13px;font-weight: 100;position: relative;color:black">` +
                    name + `</label>
            <label style="display: none" class="first ">` + id +
                    `</label>
            <span style="display:none" class="icon image1">` + icon +
                    `</span>
            <i class="fa fa-plus-square-o fa-2x pull-right" style="font-size:15px;padding-top:6px"></i>
        </a>`;

                return $allData;

            }
            // ##################################################


            var border = <%-JSON.stringify(border.active) %>;
            console.log(border);
            var $a3 = $('.button-area');
            var $a4 = $('.button-area1');
            if (border === "is-active") {
                // $("#secondswitch").trigger('click');

                var $a3 = $('.button-area');
                var $a4 = $('.button-area1');

                $a3.removeClass('rectangle');

                $a4.removeClass('rectangle');
                $a3.toggleClass('rounded');
                $a4.toggleClass('rounded');
                checkit();
            }

            function checkit() {
                var $a3 = $('.button-area');
                var $a4 = $('.button-area1');
                if ($a3.hasClass('rounded')) {
                    console.log("do nothing");

                } else {
                    $a3.addClass('rectangle');
                    $a4.addClass('rectangle');
                }
            }






        });
    </script>

